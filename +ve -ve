#include <iostream>
using namespace std;
int main() {
    int a;
    cout << "Enter your number :";
    cin>>a;
    
    if(a > 0){
        cout<<"Positive number"<<endl;
    }else if(a < 0){
        cout <<"Negative number"<<endl;
    }else if(a = 0){
        cout<<" The number is zero"<<endl;
    }
}
